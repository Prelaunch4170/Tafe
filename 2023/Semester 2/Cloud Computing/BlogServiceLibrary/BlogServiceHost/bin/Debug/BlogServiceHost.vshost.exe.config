<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="Default">
                    <serviceMetadata httpGetEnabled="true" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service behaviorConfiguration="Default" name="BlogServiceLibrary.BloggingService">
                <endpoint address="ws" binding="wsHttpBinding" contract="BlogServiceLibrary.IBloggingService" />
                <endpoint address="basic" binding="basicHttpBinding" contract="BlogServiceLibrary.IBloggingService" />
                <endpoint address="net.tcp://localhost:8388" binding="netTcpBinding"
                    contract="BlogServiceLibrary.IBloggingService" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8384/blogService" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>